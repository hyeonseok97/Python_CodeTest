# 모험가 길드

n = int(input()) # 모험가의 수를 입력
array = list(map(int, input().split())) # 모험가의 공포도를 리스트로 초기화

array.sort() # 오름차순으로 정렬
 
result = [] # 공포도에 따라 모험가를 담을 리스트
count = 0 # 그룹의 수

for i in range(len(array)):
    result.append(array[i])
    if max(result) == len(result):
        result = []
        count += 1

print(count)
